.env.example

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/hq_operations

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Authentication
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-key-here

# Stripe
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_xxxxx
STRIPE_SECRET_KEY=sk_test_xxxxx
STRIPE_WEBHOOK_SECRET=whsec_xxxxx

# AWS S3
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_S3_BUCKET_NAME=hq-operations-files
AWS_REGION=us-east-1

# Email Service (Resend)
RESEND_API_KEY=your-resend-key

# Inngest
INNGEST_API_KEY=your-inngest-key
INNGEST_EVENT_KEY=your-event-key

# AI/ML Services
OPENAI_API_KEY=your-openai-key

# External Integrations
GOODSHUFFLE_API_KEY=your-goodshuffle-key
NOWSTA_API_KEY=your-nowsta-key
KANNBAHN_API_KEY=your-kannbahn-key

# Monitoring & Analytics
SENTRY_DSN=your-sentry-dsn
NEXT_PUBLIC_POSTHOG_KEY=your-posthog-key

# Environment
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000
